<div class="weatherWidget">
    {{#each partials.wheatherContext}}
    <div class="location">
        <h3>{{location.name}}</h3>
        <a href="{{location.forecastUrl}}">
            <img src="{{iconUrl}}" alt="{{weather}}">
                {{weather}},{{temp}}
        </a>
    </div>
    {{/each}}
    <small>Source: <a
     href="https://www.weather.gov/documentation/services-web%02api">
     National weather Service</a></small>
</div>